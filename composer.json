{
    "name": "yunshu2009/mphp",
    "description": "A simple php mvc framework",
    "keywords": [
        "framework",
        "mphp"
    ],
    "homepage": "https://yunshu.me",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "yunshu2009",
            "email": "i@yunshu.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/var-dumper": "^3.1",
        "catfan/Medoo": "^1.5",
        "vlucas/phpdotenv": "^2.4",
        "monolog/monolog": "^1.23",
        "slim/slim": "^3.0",
        "php-di/slim-bridge": "^2.0",
        "hassankhan/config": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "files": [
            "support/helper.php",
            "app/common.php"
        ],
        "classmap": [
            "app/controller",
            "app/service",
            "app/dao",
            "app/repository"
        ],
        "psr-4": {
            "app\\": "app/",
            "mphp\\":"mphp/"
        }
    }
}
