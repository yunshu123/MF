{
    "name": "yunshu2009/mphp",
    "description": "A Simple PHP MVC Framework based on Composer, it helps you quickly write simple yet powerful APIs.",
    "keywords": [
        "framework",
        "mphp"
    ],
    "homepage": "https://yunshu.me",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "yunshu2009",
            "email": "i@yunshu.me"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "catfan/Medoo": "^1.5",
        "vlucas/phpdotenv": "^2.4",
        "monolog/monolog": "^1.23",
        "slim/slim": "^3.0",
        "hassankhan/config": "^1.0",
        "phpunit/phpunit": "^7.0",
        "topthink/think-validate": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "files": [
            "app/helper.php"
        ],
        "classmap": [
            "app/dao"
        ],
        "psr-4": {
            "app\\": "app"
        }
    }
}
